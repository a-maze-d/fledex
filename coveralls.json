{
    "default_stop_words": [
      "defmodule",
      "defrecord",
      "defimpl",
      "defexception",
      "defprotocol",
      "defstruct",
      "def.+(.+\\\\.+).+do",
      "^\\s+use\\s+"
    ],

    "skip_files": [
        "lib/fledex/component/thermometer.ex",
        "lib/fledex/effect/sequencer.ex",
        "test"
    ],

    "custom_stop_words": [
    ],
  
    "coverage_options": {
      "treat_no_relevant_lines_as_covered": true,
      "output_dir": "cover/",
      "minimum_coverage": 0,
      "floor_coverage": true,
      "html_filter_full_covered": true
    },
  
    "terminal_options": {
      "file_column_width": 40
    }
  }
  